{
  "name": "carrefour-mobile-tests",
  "version": "1.0.0",
  "description": "Projeto de automação mobile do desafio Carrefour",
  "main": "index.js",
"scripts": {
  "test:android": "wdio wdio.android.conf.js",
  "test:signup": "wdio wdio.android.conf.js --spec ./src/specs/signup.spec.js",
  "test:forms": "wdio wdio.android.conf.js --spec ./src/specs/forms.spec.js",
  "test:swipe:h": "wdio wdio.android.conf.js --spec ./src/specs/swipe.horizontal.spec.js",
  "test:swipe:v": "wdio wdio.android.conf.js --spec ./src/specs/swipe.vertical.spec.js",
  "test:login": "wdio wdio.android.conf.js --spec ./src/specs/login.spec.js",
  "test:drag": "wdio wdio.android.conf.js --spec ./src/specs/drag.spec.js",
  "appium": "appium",
  "allure:generate": "allure generate reports/allure-results --clean -o reports/allure-report",
  "allure:open": "allure open reports/allure-report"
},
